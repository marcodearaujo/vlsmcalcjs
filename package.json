{
  "name": "vlsmcalc",
  "version": "1.1.0",
  "description": "Lib to calc Variable Length Subnet Masking",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest test/*",
    "coverage": "jest --json --coverage --outputFile=report.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcodearaujo/vlsmcalcjs.git"
  },
  "keywords": [
    "vlsm-calc"
  ],
  "author": "Marco De Araujo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcodearaujo/vlsmcalcjs/issues"
  },
  "homepage": "https://marcodearaujo.com/",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
